{
    "name": "dreboard/example-app",
    "type": "project",
    "description": "The Basic Framework.",
    "keywords": ["framework", "dreboard"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.1",
        "cybercog/laravel-ban": "^4.7.0",
        "fruitcake/laravel-cors": "^v2.2.0",
        "guzzlehttp/guzzle": "^7.4.2",
        "lab404/laravel-impersonate": "^1.7.3",
        "laravel/framework": "^v8.83.6",
        "laravel/sanctum": "^2.15.0",
        "laravel/tinker": "^v2.7.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.6.7",
        "facade/ignition": "^2.17.5",
        "fakerphp/faker": "^v1.19.0",
        "krlove/eloquent-model-generator": "^1.3.8",
        "laravel/breeze": "^v1.9.0",
        "laravel/sail": "^v1.13.9",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^v5.11.0",
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
